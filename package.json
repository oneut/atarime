    "lint:fix": "eslint --fix --ext .js,jsx,.ts,.tsx ./src",
    "prepare": "npm run build",
    "test": "jest",
    "coverage": "npm test -- --coverage",
    "report": "nyc report --reporter=html",
    "commit": "git-cz"
  },
  "files": [
    "lib",
    "ssr"
  ],
  "peerDependencies": {
